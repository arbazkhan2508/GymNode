<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Car renter</title>
    <link href="https://cdn.jsdelivr.net/npm/remixicon@3.0.0/fonts/remixicon.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <link rel="stylesheet" href="/stylesheets/sign.css">
    <link rel="stylesheet" href="/stylesheets/nav.css">
    <link rel="stylesheet" href="/stylesheets/footer.css">
  </head>
  <body>
    
    <% include ./partials/nav.ejs %>
    <div id="main">
        <div style="margin-top: 10vh;" id="left-panel">
            <div style="margin-top: 7vh;" id="sign-in-page">
            <h1>Register your account</h1>
            <form id="land-form" action="/register" method="post">
                <input placeholder="Name" type="text" required="" name="name">
                <br>
                <input placeholder="E-mail" type="email" required="" name="email">
                <br>
                <input placeholder="Contect no:" type="text" required="" name="phoneNumber">
                <br>
                <input placeholder="Password" type="password" required="" name="password">
               
                <button type="submit">Sign Up</button>
            </form>
             <div class="login_btm">
              <a href="/login">Account Already Exist?</a>
              <div id="gitgoog">
                <a id="google_link" href="/login/federated/google"><i class="ri-google-fill"></i>Continue With Google</a>
              </div>
            </div>
        </div>
        </div>
        
    <% include ./partials/footer.ejs %>

    </div>
    
    <script src="../javascripts/nav.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
    
</body>
  
