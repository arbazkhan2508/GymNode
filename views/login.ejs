  <body>
    
    <% include ./partials/nav.ejs %>
    <div id="main">
       
        <div id="left-panel">
            
            <div id="sign-in-page">
                <h1>Login your account</h1>
                <form style="margin-top: 3vh;" id="land-form" action="/login" method="post">
                    
                    <input placeholder="E-mail" type="email" required="" name="email">
                    <br>
                    <input style="margin-bottom: 1vmax;" placeholder="Password" type="password" required="" name="password">
                    <a style="margin-right: 20vmax;margin-top: 1vmax;" href="/forgot">Forgot Password</a>
                   
                    <button type="submit">Login</button>
                </form>
                <div class="login_btm">
                    <a href="/sign">Create New Account</a>
                    <div id="gitgoog">
                        <a id="google_link" href="/login/federated/google"><i class="ri-google-fill"></i>Continue With Google</a>
                    </div>
                </div>
            </div>
        </div>
    <% include ./partials/footer.ejs %>
       
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
   
